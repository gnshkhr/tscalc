  "betterScripts": {
    "dev": {
      "command": "webpack-dev-server --inline",
      "env": {
        "NODE_ENV": "development",
        "HMR": "disabled"
      }
    },
    "dev:hot": {
      "command": "webpack-dev-server --inline --hot",
      "env": {
        "NODE_ENV": "development",
        "HMR": "enabled"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "karma start --auto-watch --no-single-run",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
